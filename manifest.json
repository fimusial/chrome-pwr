{
  "manifest_version": 3,
  "name": "Chrome PWR",
  "description": "A custom pocketknife for the Chrome",
  "version": "0.1",
  "action": {
    "default_popup": "chrome-pwr.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["blades/passive/forced-css-passive.js"]
    }
  ]
}